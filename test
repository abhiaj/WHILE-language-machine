program test::
var a,b,c,d,v,f, g,h:int;
var boo:bool;
{
	a := 6*6%5;
	g := 5*g;
	write a+b;

	if (5>3) 
		then{
			if(b<a)
			then{
				c:=3;
				d:=~3;
			}
			else{c:=8;}
			endif;
		}
		else{
			boo:=tt>ff>tt>(2>3)<=(3=4);
		}
	endif;

	v:=0;
	read boo;
	while (v < 10)
	do {
		a:=a+1; 
		v := v+1;

		if(v<5)
			then{
				while(b<=10)
				do {
					b:= b+1;
				}
				endwh;
			}
			else{}
		endif;
	}
	endwh;
	read d;
	write a+b;
}
